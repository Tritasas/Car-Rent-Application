    function confirm_add_siblings(){
        if( document.getElementById( "new_siblings_name" ).value == "" ){
            alert("ระบุ 'ชื่อ-สกุล พี่น้อง - Siblings's name'")
        }else if( document.getElementById( "new_siblings_age" ).value == "" ){
            alert("ระบุ 'อายุ - Age'")

        }else{
            let data = {
                siblings_name : document.getElementById( "new_siblings_name" ).value , 
                siblings_age : document.getElementById( "new_siblings_age" ).value , 
                siblings_position : document.getElementById( "new_siblings_position" ).value , 
                siblings_organization : document.getElementById( "new_siblings_organization" ).value 
            }

            $.ajax({
                type: 'POST',
                url: '/add_siblings',
                data: data,
                success: function (nextPage) {
                    window.location = nextPage;
                },
                error: function (error) {
                    console.log("error with Ajax");
                    console.log(error);
                }
            });
        }
    }

