        <div class='dashboard-app' style="width: 500px; margin-top: 60px !important;">
            <header class='dashboard-toolbar' style="display: flex; justify-content: space-between;">
                <a style="cursor: pointer;" class="menu-toggle">
                    <i class="fas fa-bars"></i>
                </a>
                <div class="row">
                    <div class="col-4" style="padding: 0px;">
                        <a style="cursor: pointer;" class="menu-header">
                            <i class="fa-solid fa-bell"></i>
                        </a>
                    </div>
                    <div class="col-4" style="padding: 0px;">
                        <a style="cursor: pointer;" class="menu-header" data-bs-toggle="modal" data-bs-target="#myModal">
                            <i class="fa-solid fa-book"></i>
                        </a>
                    </div>
                    <div class="col-4" style="padding: 0px;">
                        <a style="cursor: pointer;" class="menu-header" data-bs-toggle="offcanvas" data-bs-target="#offcanvasRight" aria-controls="offcanvasRight">
                            <i class="fa-solid fa-user-gear"></i>
                        </a>
                    </div>
                </div>
            </header>
            <div class='dashboard-content'>
                <div class='container'>
                    <!-- <div class="row" style="margin-bottom: 10px;">
                        <div class="col-12">
                            <button onclick="back_page()" title="กลับ" type="button" class="btn btn-secondary btn-back">
                                <i class="fa-solid fa-angle-left"></i>
                                กลับ
                            </button>
                        </div>
                    </div> -->

                    <div class="tab-content" id="v-pills-tabContent">
                        <div class="tab-pane fade" id="v-1" name="tap_body" role="tabpanel" aria-labelledby="v-1-tab">
                            <div class='card' style="box-shadow: rgba(0, 0, 0, 0.15) 0px 5px 15px;">
                                <div class='card-header'>
                                    <h3 style="display: flex; justify-content: space-between;">
                                        <div></div>
                                        ประวัติส่วนตัว - Personal Record
                                        <div id="status_save" title="SAVED">
                                            <i style="color: rgb(0, 219, 0);" class="fa-regular fa-floppy-disk"></i>
                                        </div>
                                    </h3>
                                </div>
                                <div class='card-body'>
                                    <div class="row">

                                        <div class="col-sm-12 col-md-12 col-lg-4 col-xl-2" style="margin-bottom: 15px;">
                                            <div class="parent" style="height: 100%; width: 100%; cursor: pointer; min-height: 200px;" data-bs-toggle="modal" data-bs-target="#candidate_insert_image">
                                                <!-- <div id="blah" style="background: url('assets/images/candidate.png') no-repeat;  -->

                                                <% if( candidate_data_image.length == 0 ){ %>
                                                    <div id="blah" style="background: url('https://via.placeholder.com/100') no-repeat; 
                                                    height: 100%;
                                                    width: 100%;
                                                    border: 1px solid;
                                                    background-position: center;
                                                    background-size: contain;" ></div>
                                                <% }else{ %>
                                                    <div id="blah" style="background: url('<%= candidate_data_image[0].cimg_new_name %>') no-repeat; 
                                                    height: 100%;
                                                    width: 100%;
                                                    border: 0px solid;
                                                    background-position: center;
                                                    background-size: contain;" ></div>
                                                <% } %>
                                            </div>
                                        </div>

                                        <div class="col-sm-12 col-md-12 col-lg-8 col-xl-10" style="margin-bottom: 15px;">
                                            <div class="row">
                                                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" style="margin-bottom: 15px;">
                                                    <label class="form-label">คำนำหน้า</label>
                                                    <select onchange="update_candidate_data_section_1( 'cds1_name_title' , this.value )" id="" class="selectpicker form-control" title="-- เลือก --" data-show-subtext="true" data-live-search="true">
                                                        <option selected style="display: none;" value="<%= candidate_data_section_1[0].cds1_name_title %>"><%= candidate_data_section_1[0].cds1_name_title %></option>
                                                        <option value="นาย / Mr.">นาย / Mr.</option>
                                                        <option value="นาง / Mrs.">นาง / Mrs.</option>
                                                        <option value="นางสาว / Ms.">นางสาว / Ms.</option>
                                                    </select>
                                                </div>
        
                                                <div class="col-sm-0 col-md-6 col-lg-6 col-xl-8" style="margin-bottom: 15px;">
                                                </div>
        
                                                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" style="margin-bottom: 15px;">
                                                    <label class="form-label">ชื่อ (TH)</label>
                                                    <input onchange="update_candidate_data_section_1( 'cds1_name_th' , this.value )" value="<%= candidate_data_section_1[0].cds1_name_th %>" type="text" id="" class="form-control" />
                                                </div>
        
                                                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" style="margin-bottom: 15px;">
                                                    <label class="form-label">นามสกุล (TH)</label>
                                                    <input onchange="update_candidate_data_section_1( 'cds1_surname_th' , this.value )" value="<%= candidate_data_section_1[0].cds1_surname_th %>" type="text" id="" name="" class="form-control" />
                                                </div>
        
                                                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" style="margin-bottom: 15px;">
                                                    <label class="form-label">ชื่อเล่น (TH)</label>
                                                    <input onchange="update_candidate_data_section_1( 'cds1_nickname_th' , this.value )" value="<%= candidate_data_section_1[0].cds1_nickname_th %>" type="text" id="" name="" class="form-control" />
                                                </div>
        
                                                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" style="margin-bottom: 15px;">
                                                    <label class="form-label">ชื่อ (EN)</label>
                                                    <input onchange="update_candidate_data_section_1( 'cds1_name_en' , this.value )" value="<%= candidate_data_section_1[0].cds1_name_en %>" type="text" id="" name="" class="form-control" />
                                                </div>
        
                                                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" style="margin-bottom: 15px;">
                                                    <label class="form-label">นามสกุล (EN)</label>
                                                    <input onchange="update_candidate_data_section_1( 'cds1_surname_en' , this.value )" value="<%= candidate_data_section_1[0].cds1_surname_en %>" type="text" id="" name="" class="form-control" />
                                                </div>
        
                                                <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" style="margin-bottom: 15px;">
                                                    <label class="form-label">ชื่อเล่น (EN)</label>
                                                    <input onchange="update_candidate_data_section_1( 'cds1_nickname_en' , this.value )" value="<%= candidate_data_section_1[0].cds1_nickname_en %>" type="text" id="" name="" class="form-control" />
                                                </div>
                                            </div>
                                        </div>



                                        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12" style="margin-bottom: 15px;">
                                            <hr>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">วันเดือนปีเกิด - Date of Birth</label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_birthday' , this.value )" value="<%= dayjs(candidate_data_section_1[0].cds1_birthday).format('YYYY-MM-DD') %>" type="date" id="" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">อายุ - age</label>
                                            <div class="input-group">
                                                <input onchange="update_candidate_data_section_1( 'cds1_age' , this.value )" value="<%= candidate_data_section_1[0].cds1_age %>" type="number" id="" name="" class="form-control" />
                                                <span class="input-group-text">ปี (Year)</span>
                                            </div>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">เชื้อชาติ - Nationality</label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_nationality	' , this.value )" value="<%= candidate_data_section_1[0].cds1_nationality %>" type="text" id="" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">สัญชาติ - Race</label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_race' , this.value )" value="<%= candidate_data_section_1[0].cds1_race %>" type="text" id="" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">ศาสนา - Religion</label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_religion' , this.value )" value="<%= candidate_data_section_1[0].cds1_religion %>" type="text" id="" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">กรุ๊ปเลือด - Blood Type</label>
                                            <select onchange="update_candidate_data_section_1( 'cds1_blood_type' , this.value )" id="" class="selectpicker form-control" title="-- เลือก --" data-show-subtext="true" data-live-search="true">
                                                <option selected style="display: none;" value="<%= candidate_data_section_1[0].cds1_blood_type %>"><%= candidate_data_section_1[0].cds1_blood_type %></option>
                                                <option value="A">A</option>
                                                <option value="B">B</option>
                                                <option value="AB">AB</option>
                                                <option value="O">O</option>
                                            </select>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">ส่วนสูง - Height</label>
                                            <div class="input-group">
                                                <input onchange="update_candidate_data_section_1( 'cds1_height' , this.value )" value="<%= candidate_data_section_1[0].cds1_height %>" type="number" id="" name="" class="form-control" />
                                                <span class="input-group-text">ซม. (cm)</span>
                                            </div>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">น้ำหนัก - Weight</label>
                                            <div class="input-group">
                                                <input onchange="update_candidate_data_section_1( 'cds1_weight' , this.value )" value="<%= candidate_data_section_1[0].cds1_weight %>" type="number" id="" name="" class="form-control" />
                                                <span class="input-group-text">กก. (kg)</span>
                                            </div>
                                        </div>

                                        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12" style="margin-bottom: 15px;">
                                            <hr>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
                                            <label class="form-label">สถานภาพทางทหาร - Military Service</label>
                                            <select onchange="update_candidate_data_section_1( 'cds1_military_service' , this.value )" id="military_service" class="selectpicker form-control" title="-- เลือก --" data-show-subtext="true" data-live-search="true">
                                                <option selected style="display: none;" value="<%= candidate_data_section_1[0].cds1_military_service %>"><%= candidate_data_section_1[0].cds1_military_service %></option>
                                                <option value="ผ่านการเกณฑ์ทหาร (Conscripted)">ผ่านการเกณฑ์ทหาร (Conscripted)</option>
                                                <option value="ยังไม่ผ่านการเกณฑ์ทหาร (Not Exempted)">ยังไม่ผ่านการเกณฑ์ทหาร (Not Exempted)</option>
                                                <option value="ได้รับการยกเว้น (Exempted)">ได้รับการยกเว้น (Exempted)</option>
                                            </select>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
                                            <label class="form-label">เพราะ - because <span class="red"> * ระบุกรณี ได้รับการยกเว้น (Exempted)</span></label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_military_because' , this.value )" value="<%= candidate_data_section_1[0].cds1_military_because %>" type="text" id="military_because" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
                                            <label class="form-label">อุปสมบท - Ordination</label>
                                            <select onchange="update_candidate_data_section_1( 'cds1_ordination' , this.value )" id="ordination" class="selectpicker form-control" title="-- เลือก --" data-show-subtext="true" data-live-search="true">
                                                <option selected style="display: none;" value="<%= candidate_data_section_1[0].cds1_ordination %>"><%= candidate_data_section_1[0].cds1_ordination %></option>
                                                <option value="เคย (Yes)">เคย (Yes)</option>
                                                <option value="ไม่เคย (No)">ไม่เคย (No)</option>
                                            </select>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
                                            <label class="form-label">ระยะเวลา - duration <span class="red"> * ระบุกรณี เคยอุปสมบท (Yes)</span></label>
                                            <div class="input-group">
                                                <input onchange="update_candidate_data_section_1( 'cds1_ordination_duration' , this.value )" value="<%= candidate_data_section_1[0].cds1_ordination_duration %>" type="number" id="ordination_duration" name="" class="form-control" />
                                                <span class="input-group-text">วัน (Days)</span>
                                            </div>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
                                            <label class="form-label">สถานภาพสมรส - Marital Status</label>
                                            <select onchange="update_candidate_data_section_1( 'cds1_marital_status' , this.value )" id="marital_status" class="selectpicker form-control" title="-- เลือก --" data-show-subtext="true" data-live-search="true">
                                                <option selected style="display: none;" value="<%= candidate_data_section_1[0].cds1_marital_status %>"><%= candidate_data_section_1[0].cds1_marital_status %></option>
                                                <option value="หม้าย (Widowed)">หม้าย (Widowed)</option>
                                                <option value="หย่าร้าง (Divorced)">หย่าร้าง (Divorced)</option>
                                                <option value="โสด (Single)">โสด (Single)</option>
                                                <option value="สมรส (Married)">สมรส (Married)</option>
                                            </select>
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
                                            <label class="form-label">ชื่อ-สกุลคู่สมรส - Spouse's name <span class="red"> * ระบุกรณี สมรส (Married)</span></label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_spouse_name' , this.value )" value="<%= candidate_data_section_1[0].cds1_spouse_name %>" type="text" id="spouse_name" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">เบอร์ติดต่อ - Phone No.</label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_spouse_phone' , this.value )" value="<%= candidate_data_section_1[0].cds1_spouse_phone %>" type="text" id="spouse_phone" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-3" style="margin-bottom: 15px;">
                                            <label class="form-label">อาชีพคู่สมรส - Spouse's Occupation</label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_spouse_occupation' , this.value )" value="<%= candidate_data_section_1[0].cds1_spouse_occupation %>" type="text" id="spouse_occupation" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
                                            <label class="form-label">สถานที่ทำงาน - Name of Organization</label>
                                            <input onchange="update_candidate_data_section_1( 'cds1_spouse_organization' , this.value )" value="<%= candidate_data_section_1[0].cds1_spouse_organization %>" type="text" id="spouse_organization" name="" class="form-control" />
                                        </div>

                                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-bottom: 15px;">
