    function check_input_other(){
        if( document.getElementById("military_service").value == "ได้รับการยกเว้น (Exempted)" ){
            document.getElementById("military_because").disabled = false
        }else{
            document.getElementById("military_because").disabled = true
            document.getElementById("military_because").value = ""
        }

        if( document.getElementById("ordination").value == "เคย (Yes)" ){
            document.getElementById("ordination_duration").disabled = false
        }else{
            document.getElementById("ordination_duration").disabled = true
            document.getElementById("ordination_duration").value = ""
        }

        if( document.getElementById("marital_status").value == "สมรส (Married)" ){
            document.getElementById("spouse_name").disabled = false
            document.getElementById("spouse_phone").disabled = false
            document.getElementById("spouse_occupation").disabled = false
            document.getElementById("spouse_organization").disabled = false
        }else{
            document.getElementById("spouse_name").disabled = true
            document.getElementById("spouse_name").value = ""

            document.getElementById("spouse_phone").disabled = true
            document.getElementById("spouse_phone").value = ""

            document.getElementById("spouse_occupation").disabled = true
            document.getElementById("spouse_occupation").value = ""

            document.getElementById("spouse_organization").disabled = true
            document.getElementById("spouse_organization").value = ""
        }
    }
</script>